{
	"name": "@phoria/phoria-svelte",
	"version": "0.1.0",
	"description": "Use Svelte with Phoria Islands for dotnet.",
	"keywords": [
		"phoria",
		"phoria-plugin",
		"dotnet",
		"aspnet",
		"islands",
		"vite",
		"vite-plugin",
		"svelte"
	],
	"author": "Chris Meagher",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CMeeg/phoria.git",
		"directory": "packages/phoria-svelte"
	},
	"bugs": {
		"url": "https://github.com/CMeeg/phoria/issues"
	},
	"homepage": "https://github.com/CMeeg/phoria#readme",
	"type": "module",
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"main": "./dist/main.cjs",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.cjs",
			"types": "./dist/main.d.ts"
		},
		"./client": {
			"import": "./dist/client.js",
			"require": "./dist/client.cjs",
			"types": "./dist/client/main.d.ts"
		},
		"./server": {
			"import": "./dist/server.js",
			"require": "./dist/server.cjs",
			"types": "./dist/server/main.d.ts"
		},
		"./vite": {
			"import": "./dist/vite.js",
			"require": "./dist/vite.cjs",
			"types": "./dist/vite/plugin.d.ts"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "biome check",
		"check": "tsc"
	},
	"dependencies": {
		"@rollup/pluginutils": "catalog:",
		"magic-string": "catalog:"
	},
	"devDependencies": {
		"@phoria/phoria": "workspace:*",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"svelte": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vite-plugin-dts": "catalog:",
		"vite-plugin-externalize-deps": "catalog:",
		"vite-tsconfig-paths": "catalog:"
	},
	"peerDependencies": {
		"@phoria/phoria": "^0.1.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"svelte": "^5.0.0",
		"vite": "^5.0.0"
	},
	"engines": {
		"node": "^18.17.1 || ^20.3.0 || >=21.0.0"
	}
}
