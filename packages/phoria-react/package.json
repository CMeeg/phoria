{
	"name": "@meeg/phoria-react",
	"version": "0.0.0",
	"description": "Use React with Phoria Islands for dotnet.",
	"keywords": ["phoria", "phoria-plugin", "dotnet", "aspnet", "islands", "vite", "vite-plugin", "react"],
	"author": "Chris Meagher",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CMeeg/phoria.git",
		"directory": "packages/phoria-react"
	},
	"bugs": {
		"url": "https://github.com/CMeeg/phoria/issues"
	},
	"homepage": "https://github.com/CMeeg/phoria#readme",
	"type": "module",
	"files": ["dist", "LICENSE", "README.md"],
	"main": "./dist/main.cjs",
	"module": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"exports": {
		".": {
			"import": "./dist/main.js",
			"require": "./dist/main.cjs",
			"types": "./dist/main.d.ts"
		},
		"./client": {
			"import": "./dist/client.js",
			"require": "./dist/client.cjs",
			"types": "./dist/client/main.d.ts"
		},
		"./server": {
			"import": "./dist/server.js",
			"require": "./dist/server.cjs",
			"types": "./dist/server/main.d.ts"
		},
		"./vite": {
			"import": "./dist/vite.js",
			"require": "./dist/vite.cjs",
			"types": "./dist/vite/plugin.d.ts"
		}
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production vite build",
		"lint": "biome check",
		"check": "tsc"
	},
	"dependencies": {
		"@rollup/pluginutils": "catalog:",
		"magic-string": "catalog:"
	},
	"devDependencies": {
		"@meeg/phoria": "workspace:*",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitejs/plugin-react": "^4.3.3",
		"cross-env": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vite-plugin-dts": "catalog:",
		"vite-plugin-externalize-deps": "catalog:",
		"vite-tsconfig-paths": "catalog:"
	},
	"peerDependencies": {
		"@meeg/phoria": "^0.1.0",
		"@types/react": "npm:types-react@^19.0.0-rc",
		"@types/react-dom": "npm:types-react-dom@^19.0.0-rc",
		"@vitejs/plugin-react": "^4.0.0",
		"react": "^19.0.0-rc",
		"react-dom": "^19.0.0-rc",
		"vite": "^5.0.0"
	},
	"engines": {
		"node": "^18.17.1 || ^20.3.0 || >=21.0.0"
	}
}
